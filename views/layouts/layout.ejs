        /**
         * Load images progressively:load a small blurry image first,
         * then transition to the large image
         * @param  Object  element  DOM element holding the image
         * @param  boolean isMobile True to load a smaller version
         */
        function loadImgProgressively(element, isMobile) {
          isMobile = (typeof isMobile === 'undefined') ? false : isMobile;

          var _placeholder = element.querySelector('.background.placeholder'),
              imgTiny = _placeholder.querySelector('.img-tiny');

          // 1: load small image and show it
          var _img = new Image();
          _img.src = imgTiny.src;
          _img.onload = function () {
            imgTiny.classList.add('loaded');
          };

          // 2: load large image
          var _imgLarge = new Image();
          if (isMobile) {
            _imgLarge.src = _placeholder.dataset.mid;
          }
          else {
            _imgLarge.src = _placeholder.dataset.large;
          }
          _imgLarge.onload = function () {
            _imgLarge.classList.add('loaded');
            imgTiny.classList.add('hidden');

            // 3: load animation
            if (_placeholder.dataset.anim) {
              var _anim = new Image();
              _anim.src = _placeholder.dataset.anim;
              _anim.onload = function () {
                _anim.classList.add('loaded');
              };

              _placeholder.appendChild(_anim);
            }
          };

          _placeholder.appendChild(_imgLarge);
        }

